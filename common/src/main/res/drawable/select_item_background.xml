<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#FFF7DF" />
            <stroke android:width="@dimen/dp_1" android:color="@color/gray" />
            <padding android:bottom="@dimen/dp_2" android:left="@dimen/dp_2" android:right="@dimen/dp_2" android:top="@dimen/dp_2" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke android:width="@dimen/dp_1" android:color="@color/gray" />
        </shape>
    </item>
</selector>